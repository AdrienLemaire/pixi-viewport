{
  "name": "pixi-viewport",
  "version": "1.0.0",
  "description": "A scrollable, zoomable stage for PIXI.js",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "budo --dir docs docs/code.js:index.js --live",
    "build": "browserify docs/code.js -o code/index.js",
    "docs": "markdown-api README.md index.js",
    "prepare": "npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidfig/pixi-viewport.git"
  },
  "keywords": [
    "PIXI",
    "pixi.js",
    "viewport",
    "zoom",
    "scroll",
    "pinch",
    "stage"
  ],
  "author": "David Figatner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidfig/pixi-viewport/issues"
  },
  "homepage": "https://github.com/davidfig/pixi-viewport#readme",
  "dependencies": {
    "pixi-ease": "^0.2.0"
  },
  "devDependencies": {
    "fork-me-github": "^1.0.2",
    "highlight.js": "^9.12.0",
    "pixi.js": "^4.5.5",
    "yy-random": "^1.5.0"
  }
}
